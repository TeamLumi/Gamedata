ev_l02r0301_dray:
	_TALK_OBJ_START_TURN_NOT()
	_SYS_FLAG_GAME_CLEAR_GET(@LOCALWORK0)
	_IFVAL_JUMP(@LOCALWORK0, 'EQ', 0, 'ev_l02r0301_dray_come_back_later')
	_IF_FLAGON_JUMP(#2757, 'ev_l02r0301_dray_beaten')
	_MACRO_TALKMSG('dp_scenario3', '00_msg_l02r0301_dray_1', 'Congratulations on becoming\nthe Champion in Renegade Platinum!\rYour team must be super strong, now.\nDo you want to have a battle?')
	_YES_NO_WIN(@SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_l02r0301_dray_yes')
	_JUMP('ev_l02r0301_dray_no')
ev_l02r0301_dray_come_back_later:
	_MACRO_TALKMSG('dp_scenario3', '00_msg_l02r0301_dray_2', 'Hey! Call me Dray.\nI created Renegade Platinum.\rAre you having fun playing\nLuminescent Platinum?\rCome back if you become the Champion.\nWe can have a fun battle together.')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_l02r0301_dray_no:
	_MACRO_TALKMSG('dp_scenario3', '00_msg_l02r0301_dray_3', 'No worries. Come back any time.')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_l02r0301_dray_yes:
	_MACRO_TALKMSG('dp_scenario3', '00_msg_l02r0301_dray_4', 'Alright, great.\nLet’s get started!')
	_TALK_CLOSE()
	_LDVAL(@SCWK_TEMP3, 949)
	_CALL('ev_set_trainer_id_for_current_gamemode_scripted_battle')
	_TRAINER_BTL_SET(@SCWK_TEMP3, 0)
	_MACRO_TALKMSG('dp_scenario3', '00_msg_l02r0301_dray_5', 'Oof! You’re pretty good.\nI didn’t expect you’d be so strong!\rThanks for playing Renegade Platinum.\nI hope you found it to be a lot of fun!')
	_TALK_CLOSE()
	_FLAG_SET(#2757)
	_TALK_OBJ_END()
	_END()
ev_l02r0301_dray_beaten:
	_MACRO_TALKMSG('dp_scenario3', '00_msg_l02r0301_dray_6', 'Hey!\nIt’s good to see you.\rThanks for playing Luminescent Platinum.\nI hope you found it to be a lot of fun!')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()