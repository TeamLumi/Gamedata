ev_c04pc0101_jirachi_event:
	_TALK_OBJ_START()
	_IF_FLAGON_JUMP(#2669, 'ev_c04pc0101_jirachi_event_done')
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_c04pc0101_jirachi_event_1', 'Hello, Trainer!\nI have a dream.\rCould you help my Clefairy\nlearn Dazzling Gleam?')
	_YES_NO_WIN(@SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 1, 'ev_c04pc0101_jirachi_event_no')
	_ITEM_CHK(412, 1, @SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_c04pc0101_jirachi_event_no')
	_PLAYER_NAME(0)
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_c04pc0101_jirachi_event_2', '{0} booted up the TM.\r...\r...')
	_SE_PLAY('M_FI014')
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_c04pc0101_jirachi_event_3', 'Clefairy learned\nDazzling Gleam!')
	_TALK_CLOSE()
	_VOICE_PLAY(35, 0, 0)
	_VOICE_WAIT()
	_WHITE_OUT('SCR_WIPE_DIV', 6)
	_FADE_WAIT()
	_TIME_WAIT(20, @SCWK_ANSWER)
	_VOICE_PLAY(385, 0, 0)
	_WHITE_IN()
	_FADE_WAIT()
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_c04pc0101_jirachi_event_4', 'Someone, somewhere, saw that you\nmade somebody’s wish come true.')
	_FLAG_SET(#2669)
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_c04pc0101_jirachi_event_no:
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_c04pc0101_jirachi_event_no', 'Oh...\rYou can’t teach my Clefairy\nDazzling Gleam?\rOr you simply don’t want to?\rSigh...\nWill my Clefairy ever sparkle?')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_c04pc0101_jirachi_event_done:
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_c04pc0101_jirachi_event_done', 'Ah! My dream has been realized!\nThank you ever so much, Trainer!')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
