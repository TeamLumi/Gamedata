ev_c10r0102_flag_change:
	_SET_SYS_FLAG($SYS_BOX_SYSTEM_OFF)
	_LDVAL(@LOCALWORK0, 0)
	_LDVAL(@WK_SCENE_CL_DOOR, 0)
	_INIT_FLD_LIFT()
	_CHECK_FLD_LIFT(@LOCALWORK1)
	_IFVAL_JUMP(@LOCALWORK1, 'EQ', 0, 'ev_c10r0102_lift_event_off')
	_END()
ev_c10r0102_lift_event_off:
	_LDVAL(@LOCALWORK0, 1)
	_END()
pos_c10r0102_lift:
	_MOVE_FLD_LIFT()
	_LDVAL(@LOCALWORK0, 1)
	_END()