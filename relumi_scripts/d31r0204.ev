ev_tower_wayd_common:
	_EVENT_START()
	_CALL('ev_tower_wayd_comanm_in')
	_CALL('ev_tower_wayd_comanm_walk')
	_CALL('ev_tower_wayd_comanm_out')
	_MAP_CHANGE(341, 0, 'SB_ROOMD_PLAYER_SX', 'SB_ROOMD_PLAYER_SY', 0)
	_TALK_END()
	_END()
ev_tower_wayd_comanm_in:
	_SEQ_OPEN_DOOR(77)
