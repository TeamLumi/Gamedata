	_WAIT_3D_ANIME(77)
	_WAIT_3D_ANIME(78)
	_OBJ_ANIME('TOWER_GATE_03', 'anm_d31r0204_pcwoman2_in')
	_OBJ_ANIME('TOWER_GATE_04', 'anm_d31r0204_pcwoman2_in')
	_OBJ_ANIME_WAIT()
	_OBJ_VISIBLE('TOWER_PLAYER_00_01')
	_OBJ_VISIBLE('TOWER_PLAYER_01_01')
