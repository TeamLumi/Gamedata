	_OBJ_ANIME('TOWER_PLAYER_01_01', 'anm_d31r0204_trainer_in')
	_OBJ_ANIME_WAIT()
	_SEQ_CLOSE_DOOR(77)
	_SEQ_CLOSE_DOOR(78)
	_WAIT_3D_ANIME(77)
	_WAIT_3D_ANIME(78)
	_FREE_3D_ANIME(77)
	_FREE_3D_ANIME(78)
	_RET()
