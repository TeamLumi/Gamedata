ev_d12r0101_onix:
	_TALK_OBJ_START()
	_CAMERA_SHAKE(12, 1, 0, 15, 180)
	_VOICE_PLAY(95, 0, 0)
	_VOICE_WAIT()
	_SP_WILD_BTL_SET(95, 50, 0, 1)
	_SEACRET_POKE_RETRY_CHECK(@SCWK_TEMP0, 95); Get result
	_IFVAL_JUMP(@SCWK_TEMP0, 'EQ', 0, 'ev_d12r0101_onix_lose');CatchChk
	_OBJ_DEL('D12R0101_CRYSTAL_ONIX')
	_FLAG_SET(#2874)
	_FLAG_SET(#2875)
	_OBJ_DEL('D12R0101_GRAVELLER_1')
	_FLAG_SET(#2876)
	_OBJ_DEL('D12R0101_GRAVELLER_2')
	_FLAG_SET(#2877)
	_OBJ_DEL('D12R0101_GRAVELLER_3')
	_JUMP('ev_d12r0101_onix_beaten')
	_END()
