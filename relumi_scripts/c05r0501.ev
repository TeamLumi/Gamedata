	_YES_NO_WIN(@SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_c05_aaron_yes_battle')
	_JUMP('ev_c05_aaron_no_battle')
