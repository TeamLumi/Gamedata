pos_c05r0501_aaron_trigger:
	_TALK_OBJ_START()
	_FLAG_RESET(#2853); This is the Placedata for Aaron's Chibi.
	_OBJ_ADD('C05R0501_AARON')
	_LDVAL(@489, 1); This is the flag for the Stopdata Trigger.
	_PLAYER_POS_GET(@SCWK_TEMP0, @SCWK_TEMP1); Get's the player position and uses it in ev_c05r0501_aaron_decide_path_intro
	_OBJ_ANIME('C05R0501_AARON', 'anm_c05r0501_aaron_better')
	_OBJ_ANIME_WAIT()
	_CALL('ev_c05r0501_aaron_decide_path_intro')
