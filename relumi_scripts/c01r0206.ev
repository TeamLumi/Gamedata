ev_c01r0206_flag_change:
	_FLAG_RESET(#FV_C01R0206_STOP)
	_FLAG_SET(#FH_OBJ02)
	_IF_FLAGON_JUMP($SYS_FLAG_WILDCARD_001, 'ev_c01r0206_record_open')
	_SXY_POS_CHANGE('C01R0206_GINESS01', 1032, 1051)
	_SXY_POS_CHANGE('C01R0206_GINESS02', 1032, 1052)
	_SXY_POS_CHANGE('C01R0206_GINESS03', 1032, 1053)
	_END()
ev_c01r0206_record_open:
	_IFVAL_JUMP(@WK_SCENE_C11R0101, 'EQ', 1, 'ev_c01r0206_fc_zenkoku_yes')
	_IF_FLAGOFF_JUMP($SYS_FLAG_WILDCARD_000, 'ev_c01r0206_fc_zenkoku_not')
	_END()
ev_c01r0206_fc_zenkoku_yes:
	_FLAG_SET(#FV_C01R0206_STOP)
	_OBJ_DEL('C01R0206_STOP_01')
	_FLAG_RESET(#FH_OBJ02)
	_OBJ_ADD('C01R0206_GINESS02')
	_END()
ev_c01r0206_fc_zenkoku_not:
	_END()
