ev_d09r0101_nurse:
	_TALK_OBJ_START()
	_MACRO_TALKMSG('dp_scenario3', '00_msg_ev_d09r0101_nurse_1', 'Hello, Trainer!\nAllow me heal your Pokémon.')
	_BLACK_OUT('SCR_WIPE_DIV', 3)
	_FADE_WAIT()
	_PC_KAIFUKU()
	_SE_PLAY('M_FI002')
	_ME_WAIT()
	_BLACK_IN('SCR_WIPE_DIV', 3)
	_FADE_WAIT()
