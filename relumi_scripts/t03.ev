ev_t03_ranger_drifloon_quest:
	_TALK_OBJ_START()
	_IF_FLAGON_JUMP(#2011, 'ev_t03_ranger_quest_completed'); TODO Find other flags to use :P
	_IF_FLAGON_JUMP(#2010, 'ev_t03_ranger_gift_again')
	_IF_FLAGON_JUMP(#2009, 'ev_t03_ranger_drifloon_check')
	_MACRO_TALKMSG('dp_scenario3', 'msg_t03_ranger_drifloon_start', 'Greetings! Recently, there have been reports\n of a peculiar balloon-like Pokémon\rsomewhere around here.\rIf you find that Pokémon,\ncould you please show it to me?')
	_FLAG_SET(#2009); Drifloon Ranger has been talked to once
	_TALK_CLOSE()
	_TALK_OBJ_END()
ev_t03_ranger_drifloon_check:
	_MACRO_TALKMSG('dp_scenario3', 'msg_t03_ranger_drifloon_cont', 'Oh, hello again.\rHave you found the balloon Pokémon yet?')
	_TALK_CLOSE()
	_CALL('ev_t03_ranger_pokemon_check'); Loads SCWK_PARAM1 as MonsNo and SCWK_PARAM2 as FormNo
	_MACRO_TALKMSG('dp_scenario3', 'msg_t03_ranger_drifloon_success', 'That must be it!\nEverything matches the report perfectly!\rThis species of Pokémon is a danger to\nchildren and other short people.\rIts excitement causes it to get carried away\nand well... it carries them away!\rHere, this is to thank you for\nyour help with this.')
	_JUMP('ev_t03_ranger_drifloon_success')
	_END()
ev_t03_ranger_pokemon_check:
    _POKELIST_SET_PROC(6); This only highlight a specific list of mons detailed in UIDatabase.BoxOpenParam
  	_POKELIST_GET_RESULT(@SCWK_PARAM0, @SCWK_PARAM3); Argument 1 is the bag/box position, Argument 2 is the box number (0 = Bag)
	_IFVAL_JUMP(@SCWK_PARAM0, 'EQ', -1, 'ev_t03_ranger_drifloon_no'); If SCK_PARAM0 is -1, it means you chose nothing.
    _TEMOTI_BOX_MONSNO(@SCWK_PARAM0, @SCWK_PARAM3, @SCWK_PARAM1); Get the MonsNo of the selected Pokemon, SCWK_PARAM1
	_SWITCH(@SCWK_PARAM1)
	_CASE_JUMP(425, 'ev_t03_ranger_correct_pokemon'); Drifloon
	_CASE_JUMP(0, 'ev_t03_ranger_drifloon_invalid'); Egg
	_JUMP('ev_t03_ranger_drifloon_invalid'); Everything else
    _END()
ev_t03_ranger_drifloon_no:
	_MACRO_TALKMSG('dp_scenario3', 'msg_t03_ranger_drifloon_no', 'You haven’t found it yet?\nKeep looking!\rRemember, we’re looking for a\nballoon-shaped Pokémon.')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_t03_ranger_drifloon_invalid:
	_MACRO_TALKMSG('dp_scenario3', 'msg_t03_ranger_drifloon_invalid', 'No, that’s not quite right...\rThe Pokémon we’re looking for is\nshaped like a balloon.\rThe reports also said that it’s purple,\nmaybe that will help. Good luck!')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_t03_ranger_correct_pokemon:
	_RET(); Returns back to ev_t03_ranger_drifloon_check
ev_t03_ranger_drifloon_success:
	_LDVAL(@SCWK_TEMP3, 0)
	_FIRST_POKE_NO_GET(@SCWK_ANSWER)
	_IFVAL_CALL(@SCWK_ANSWER, 'EQ', 0, 'ev_t03_ranger_gift_not_turtwig'); You have Turtwig
	_IFVAL_CALL(@SCWK_ANSWER, 'EQ', 1, 'ev_t03_ranger_gift_not_chimchar'); You have Chimchar
	_IFVAL_CALL(@SCWK_ANSWER, 'EQ', 2, 'ev_t03_ranger_gift_not_piplup'); You have Piplup
	_IFVAL_CALL(@SCWK_TEMP3, 'EQ', 1, 'ev_t03_ranger_gift_singular')
	_IFVAL_CALL(@SCWK_TEMP3, 'EQ', 2, 'ev_t03_ranger_gift_multiple')
	_MACRO_ADD_CUSTUM_WIN_LABEL('dp_options', 'DP_ranger_options_Cancel', 'Not right now', 2)
	_OPEN_CUSTUM_WIN(@SCWK_TEMP0); SCWK_TEMP0 will have the MonsNo of the pokemon chosen
	_IFVAL_JUMP(@SCWK_TEMP0, 'EQ', 2, 'ev_t03_ranger_no_gift_give')
	_POKEMON_NAME_EXTRA(0, @SCWK_TEMP0, 0, 0)
    _ME_PLAY('M_FI013')
	_TALKMSG('dp_scenario2%msg_t03_receive_starter')
    _ME_WAIT()
	_ADD_POKEMON_UI(@SCWK_TEMP0, 5, 0, @SCWK_ANSWER, 3)
	_SWITCH(@SCWK_TEMP0)
	_CASE_JUMP(387, 'ev_t03_ranger_drifloon_turtwig_flag_set')
	_CASE_JUMP(390, 'ev_t03_ranger_drifloon_chimchar_flag_set')
	_CASE_JUMP(393, 'ev_t03_ranger_drifloon_piplup_flag_set')
	_END()
