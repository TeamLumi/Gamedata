ev_d16_post_stark_event:
	_TALK_OBJ_START()
	_MACRO_TALKMSG('dp_scenario3', '00_msg_d16_text_3', 'Whew! The air outside, it is\ndelicious, yes!\r...Gweh! Gah! Geff! Bleah!\rI have inhaled the volcanic ash by\nthe lungful!')
	_TALK_CLOSE()
	_OBJ_ANIME('D16_LOOKER', 'anm_d16_looker_1'); OW 4
	_OBJ_ANIME('HERO', 'anm_d16_hero_1')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_d16_text_4', 'Now, you two.\nThis is what they were after.\rThe Magma Stone.\rMoving this is said to awaken the\nPokémon Heatran.\rThe old tales say that its awakening,\nit will make Stark Mountain erupt.\r...But I have moved it. It is here.\nWill this cause the eruption?!')
	_OBJ_ANIME('D16_BUCK_POST', 'anm_d16_buck_1'); OW 5
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_d16_text_5', 'Hey, don’t sweat it, stranger.\rGive me that Magma Stone.\nI’ll go put it back for you.')
	_TALK_CLOSE()
	_OBJ_ANIME('HERO', 'anm_d16_hero_2')
	_OBJ_ANIME('D16_BUCK_POST', 'anm_d16_buck_2')
	_OBJ_ANIME_WAIT()
	_OBJ_ANIME('D16_LOOKER', 'anm_d16_looker_2'); OW 4
	_OBJ_ANIME('HERO', 'anm_d16_hero_3')
	_OBJ_ANIME_WAIT()
	_PLAYER_NAME(0)
	_TALKMSG('dp_scenario3%00_msg_d16_text_6')
	_TALK_CLOSE()
	_TIME_WAIT(30, @SCWK_ANSWER)
	_OBJ_ANIME('D16_BUCK_POST', 'anm_d16_buck_3')
	_OBJ_ANIME('D16_LOOKER', 'anm_d16_looker_3')
	_OBJ_ANIME_WAIT()
	_PLAYER_NAME(0)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_d16_text_7', 'Hey, stranger!\nI put the Magma Stone where it belongs.\rIt’ll be OK now.\nHeatran won’t wake up. Probably.\rBut, hey, {0}!\nYou’re an amazing Trainer!\rSwing by my place later.\nIt’s in the Survival Area.\rBe seeing you, stranger, {0}!\nBye-bye!')
	_TALK_CLOSE()
	_OBJ_ANIME('D16_BUCK_POST', 'anm_d16_buck_4')
	_OBJ_ANIME('HERO', 'anm_d16_hero_4')
	_OBJ_ANIME_WAIT()
	_OBJ_DEL('D16_BUCK_POST')
	_OBJ_ANIME('D16_LOOKER', 'anm_d16_looker_4')
	_OBJ_ANIME('HERO', 'anm_d16_hero_5')
	_OBJ_ANIME_WAIT()
	_PLAYER_NAME(0)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_d16_text_8', 'I, also, will take my leave.\r“The more wonderful the meeting, the\nsadder the parting.”\rThat is a quote I have heard many years\nago in a foreign land.\rIts meaning...\nI understand it better now.\rFarewell, {0}, my friend!\nLong may you cherish Pokémon!')
	_TALK_CLOSE()
	_BLACK_OUT()
	_FADE_WAIT()
	_OBJ_DEL('D16_LOOKER')
	_TIME_WAIT(30, @SCWK_ANSWER)
	_BLACK_IN()
	_FADE_WAIT()
	_FLAG_SET(#2885)
	_LDVAL(@481, 4)
	_FLAG_SET(#2538); Post stark event done
	_FLAG_RESET(#2886)
	_TALK_OBJ_END()
	_END()
ev_t06_temp_buck:
	_IF_FLAGOFF_JUMP(#2537, 'ev_t06_temp_buck_dialogue'); has this buck up but hasn't done the Stark mountain event somehow
	_ITEM_CHK(458, 1, @SCWK_ANSWER)
    _IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_t06_magma_stone_fix')
	_JUMP('ev_t06_temp_buck_dialogue')
	_END()
ev_t06_magma_stone_fix:
	_TALK_OBJ_START()
	_LDVAL(@SCWK_TEMP0, 458); Magma Stone
	_LDVAL(@SCWK_TEMP1, 1); Quantity 1
	_CALL('ev_item_event')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_RET()
ev_t06_temp_buck_dialogue:
	_PLAYER_NAME(0)
	_MACRO_EASY_OBJ_MSG('dp_scenario3', '00_msg_t06_temp_buck', 'Hey {0}!\rWe’re still getting things ready\nin here, but the good news is that\fHeatran is now ready to catch.')
	_FLAG_SET(#FE_D16R0103_LEGEND_UNSEAL)
	_END()
