	_AC_LEFT(9, 8)
	_AC_DIR_U(8)
	_AC_UP(4, 8)
	_ACMD_END()
pos_d16_grunt_event:
	_TALK_OBJ_START()
	_CHARA_LOOK_LOCK('D16_GRUNT1', 'D16_GRUNT2')
	_CHARA_LOOK_LOCK('D16_GRUNT2', 'D16_GRUNT1')
	_MACRO_TALKMSG('dp_scenario3', '00_msg_d16_grunt_0', 'This is it!\nStark Mountain!\rNow we need to get deep inside.\nLet’s do this!')
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_d16_grunt_1', 'Yeah, but...\nCan that old guy really take over?\fCan he replace Master Cyrus?')
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_d16_grunt_2', 'Yeah, I hear you...\rThat elder Charon is a Commander,\nbut he can’t replace Master Cyrus.\r...Maybe this is a good time for me to\nbe done with this Team Galactic thing...')
	_TALK_CLOSE()
	_CHARA_LOOK_RELEASE('D16_GRUNT1')
	_CHARA_LOOK_RELEASE('D16_GRUNT2')
	_OBJ_ANIME('D16_GRUNT1', 'anm_d16_grunt_enter_stark_mountain')
	_OBJ_ANIME('D16_GRUNT2', 'anm_d16_grunt_enter_stark_mountain')
	_OBJ_ANIME_WAIT()
	_FLAG_SET(#2533)
	_OBJ_DEL('D16_GRUNT1')
	_OBJ_DEL('D16_GRUNT2')
	_LDVAL(@481, 2)
	_TALK_OBJ_END()
	_END()
