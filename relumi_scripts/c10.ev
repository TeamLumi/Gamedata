	_NONE_USE_NUMBER()
	_IFVAL_JUMP(@LOCALWORK0, 'NE', 0, 'ev_c10_townmapreset1')
	_JUMP('ev_c10_flag_change_townmap_off')
	_END()
ev_c10_townmapreset1:
	_IF_FLAGON_JUMP(#FE_C10R0101_REACH, 'ev_c10_flag_change_townmap_on')
	_JUMP('ev_c10_flag_change_townmap_off')
	_END()
