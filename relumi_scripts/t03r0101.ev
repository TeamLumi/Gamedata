ev_t03r0101_woman1:
	_TALK_OBJ_START()
	_IF_FLAGON_JUMP(#TMFLG_T03R0101_SEED_PRESENT, 'ev_t03r0101_woman1_02')
	_TALKMSG('dp_scenario1%95-msg_t03r0101_woman1_01')
	_GET_RND(@SCWK_TEMP0, 5)
	_ADD_WK(@SCWK_TEMP0, 149)
	_LDVAL(@SCWK_TEMP1, 1)
	_ADD_ITEM_CHK(@SCWK_TEMP0, @SCWK_TEMP1, @SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_t03r0101_woman1_item_event_fail')
	_FLAG_SET(#TMFLG_T03R0101_SEED_PRESENT)
	_CALL('ev_item_event')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_t03r0101_woman1_02:
	_TALKMSG('dp_scenario1%95-msg_t03r0101_woman1_02')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_t03r0101_woman1_item_event_fail:
	_CALL('ev_item_event_fail')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_t03r0101_girl1:
	_TALK_OBJ_START()
	_IF_FLAGON_JUMP(#FE_FLOWER_ITEM_GET, 'ev_t03_t03r0101_02')
	_TALKMSG('dp_scenario1%95-msg_t03r0101_girl1_01')
