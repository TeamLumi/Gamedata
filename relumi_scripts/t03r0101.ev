	_LDVAL(@SCWK_TEMP0, 448)
	_LDVAL(@SCWK_TEMP1, 1)
	_FLAG_SET(#FE_FLOWER_ITEM_GET)
	_CALL('ev_item_event')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_t03_t03r0101_02:
