ev_c10r0113_dendou:
	_EVENT_START()
	_TEMOTI_BALL_LOAD()
	_IF_FLAGON_JUMP($SYS_DENDOU_OFF, 'ev_c10r0113_end')
	_CALL('ev_box_close')
	_CALL('ev_c10r0113_custom_flags'); Run the post-E4 flags, like legendary events
	_OBJ_ANIME('HERO', 'anm_c10r0113_player_200')
	_OBJ_ANIME_WAIT()
	_OBJ_ANIME('C10R0113_DOCTOR', 'anm_c10r0113_doctor_120')
	_OBJ_ANIME_WAIT()
	_DENDOU_NUM_GET(@SCWK_ANSWER)
	_IFVAL_CALL(@SCWK_ANSWER, 'EQ', 0, 'ev_c10r0113_dendou_on')
	_IFVAL_CALL(@SCWK_ANSWER, 'NE', 0, 'ev_c10r0113_dendou_off')
	_TALK_CLOSE()
	_OBJ_ANIME('C10R0113_DOCTOR', 'anm_c10r0113_doctor_100')
	_OBJ_ANIME('C10R0113_CHAMP', 'anm_c10r0113_champ_100')
	_OBJ_ANIME_WAIT()
	_TIME_WAIT(15, @SCWK_ANSWER)
	_GET_MY_SEX(@SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_c10r0113_dendou_male')
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 1, 'ev_c10r0113_dendou_female')
	_END()
ev_c10r0113_dendou_male:
	_PLAYER_NAME(0)
	_TALKMSG('dp_scenario3%26-msg_c10r0113_champion_03')
	_JUMP('common_c10r0113_dendou')
	_JUMP('common_c10r0113_dendou')
ev_c10r0113_dendou_female:
	_PLAYER_NAME(0)
	_TALKMSG('dp_scenario3%26-msg_c10r0113_champion_04')
	_JUMP('common_c10r0113_dendou')
	_JUMP('common_c10r0113_dendou')
common_c10r0113_dendou:
	_TALK_CLOSE()
	_EVENT_CAMERA_INDEX(236)
	_OBJ_ANIME('HERO', 'anm_c10r0113_player_01')
	_OBJ_ANIME('C10R0113_DOCTOR', 'anm_c10r0113_doctor_01')
	_OBJ_ANIME('C10R0113_CHAMP', 'anm_c10r0113_champ_01')
	_OBJ_ANIME_WAIT()
	_TALKMSG('dp_scenario3%26-msg_c10r0113_champion_05_1')
	_TALKMSG('dp_scenario3%26-msg_c10r0113_champion_05_2')
	_TALK_CLOSE()
	_SET_SYS_FLAG($SYS_FLAG_BS_LV4)
	_ZENKOKU_ZUKAN_FLAG(2, @SCWK_ANSWER)
	_IFVAL_CALL(@SCWK_ANSWER, 'EQ', 1, 'ev_c10r0113_zenkoku_zukan_open')
	_EVENT_CAMERA_INDEX(234)
	_EVENT_CAMERA_END_WAIT()
	_IFVAL_CALL(@WK_SCENE_DENDOU, 'EQ', 0, 'ev_c10r0113_dendou_wk')
	_EVENT_CAMERA_INDEX(235)
	_GET_POKE_COUNT(@SCWK_ANSWER)
	_JUMP('ev_c10r0113_dendou_ball')
	_END()
ev_c10r0113_dendou_ball_end:
	_BLACK_OUT('SCR_WIPE_DIV', 'SCR_WIPE_SYNC*3')
	_FADE_WAIT()
	_TIME_WAIT(5, @SCWK_ANSWER)
	_CALL('ev_block_doc_end')
	_POKECEN_CLEAR_BALL()
	_RECORD_INC(1)
	_CALL('ev_c10r0113_game_clear_timing_flag_set')
	_DENDOU_SET_PROC()
	_ENDING_DEMO()
	_TALK_END()
	_END()
ev_c10r0113_dendou_wk:
	_LDVAL(@WK_SCENE_DENDOU, 1)
	_RET()
ev_c10r0113_zenkoku_zukan_open:
	_LDVAL(@HIDEMAP_ID_L04, 1)
	_FLAG_SET(#FV_R214_KAKURE_ROAD)
	_RET()
ev_c10r0113_dendou_on:
	_EVENT_CAMERA_INDEX(233)
	_TIME_WAIT(10, @SCWK_ANSWER)
	_SET_FORCE_BLINK('C10R0113_DOCTOR', 1)
	_TIME_WAIT(30, @SCWK_ANSWER)
	_SET_FORCE_BLINK('C10R0113_DOCTOR', 0)
	_TIME_WAIT(10, @SCWK_ANSWER)
	_TALKMSG('dp_scenario3%26-msg_c10r0113_doctor_01')
	_TALKMSG('dp_scenario3%26-msg_c10r0113_champion_01')
	_RET()
ev_c10r0113_dendou_off:
	_EVENT_CAMERA_INDEX(233)
	_TIME_WAIT(10, @SCWK_ANSWER)
	_SET_FORCE_BLINK('C10R0113_DOCTOR', 1)
	_TIME_WAIT(30, @SCWK_ANSWER)
	_SET_FORCE_BLINK('C10R0113_DOCTOR', 0)
	_TIME_WAIT(10, @SCWK_ANSWER)
	_TALKMSG('dp_scenario3%26-msg_c10r0113_doctor_02')
	_TALKMSG('dp_scenario3%26-msg_c10r0113_champion_02')
	_RET()
