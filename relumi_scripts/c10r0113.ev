	_AC_DIR_U(4)
	_AC_UP(7, 8)
	_ACMD_END()
anm_c10r0113_player_01:
	_AC_DIR_U(4)
	_AC_UP(2, 8)
	_ACMD_END()
anm_c10r0113_doctor_120:
	_AC_DIR_U(8)
	_AC_WAIT(16)
	_AC_DIR_R(8)
	_ACMD_END()
anm_c10r0113_doctor_100:
	_AC_DIR_D(8)
	_ACMD_END()
anm_c10r0113_doctor_01:
	_AC_WAIT(8)
	_AC_DIR_R(8)
	_AC_DIR_U(8)
	_ACMD_END()
anm_c10r0113_champ_100:
	_AC_DIR_D(8)
	_ACMD_END()
anm_c10r0113_champ_01:
	_AC_WAIT(8)
	_AC_DIR_L(8)
	_AC_DIR_U(8)
	_ACMD_END()
