ev_c10r0113_dendou_ball:
	_GET_POKE_COUNT2(@SCWK_TEMP2)
	_LDVAL(@SCWK_TEMP0, 0)
	_JUMP('ev_c10r0113_dendou_ball_anim_common_loop2')
	_END()
ev_c10r0113_dendou_ball_anim_common_loop:
	_JUMP('ev_c10r0113_dendou_ball_anim_common_loop2')
	_END()
