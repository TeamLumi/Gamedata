	_POKECEN_PUT_BALL(@SCWK_TEMP0)
	_TIME_WAIT(15, @SCWK_ANSWER)
	_ADD_WK(@SCWK_TEMP0, 1)
	_IFVAL_JUMP(@SCWK_TEMP0, 'EQ', @SCWK_TEMP2, 'ev_c10r0113_dendou_ball_anim_common_loop3')
