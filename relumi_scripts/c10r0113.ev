	_PC_RECOVER_ANM(@SCWK_TEMP2)
	_TIME_WAIT(15, @SCWK_ANSWER)
	_PC_RECOVER_ANM(@SCWK_TEMP2)
	_TIME_WAIT(15, @SCWK_ANSWER)
	_PC_RECOVER_ANM(@SCWK_TEMP2)
	_TIME_WAIT(15, @SCWK_ANSWER)
	_PC_RECOVER_ANM(@SCWK_TEMP2)
	_TIME_WAIT(15, @SCWK_ANSWER)
	_TIME_WAIT(4, @SCWK_ANSWER)
